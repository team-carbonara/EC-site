<div class="l-main_container">
<div class='row'>

  <div>会員一覧</div>
    <table class="table table-hover table-inverse col-sm-6 col-sm-offset-6 mx-auto">
      <thead>
        <tr class="table-info">
          <td>会員ID</td>
          <td>氏名</td>
          <td>メールアドレス</td>
          <td>ステータス</td>
        </tr>
        <% @customers.each do |customer| %>
          <tr>
            <td><%= customer.id %></td>
            <td>
              <%= link_to admin_customer_path(customer) do %>
              <%= customer.last_name %><%= customer.first_name %>
              <% end %>
            </td>
            <td><%= customer.email %></td>

            <% if customer.withdrawal_status == true%>
              <td>退会</td>
            <% else %>
              <td>有効</td>
            <% end %>

          </tr>
        <% end %>
      </thead>
    </table>
</div>
